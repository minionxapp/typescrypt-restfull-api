 //schema.prisma

model Dev_project {
id         Int    @id @default(autoincrement())
name String @db.VarChar(100)
desc String @db.VarChar(100)
 create_by   String  @db.VarChar(20)
update_by   String?  @db.VarChar(20)
create_at   DateTime? 
update_at   DateTime? 
@@map("dev_projects")
}
